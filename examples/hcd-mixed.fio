[global]
ioengine=libaio
direct=1
bs=4k

#directory=/mount-point-of-ssd
#filename=/dev/sdd:/dev/sde:/dev/sdf:/dev/sdg:/dev/sdh

filename=/dev/XXXXX
group_reporting

[rand-read]
numjobs=4
rw=randread
bs=4k
iodepth=8
runtime=5
time_based

[rand-rw]
stonewall
rw=randrw
bs=4k
numjobs=4
iodepth=8
runtime=30m
rwmixread=80

# disable verify when #jobs > 1, otherwise they may overwrite each other.

do_verify=0
verify=crc32c
verify_backlog=8
verify_async=2
#verify=md5
#verify_dump=1
